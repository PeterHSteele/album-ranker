{
  "name": "ranker",
  "version": "1.0.0",
  "description": "What is CodeIgniter ###################",
  "main": "index.js",
  "scripts": {
    "build-task:scss-compile": "node-sass-chokidar --source-map true assets/scss/ -o dist/css",
    "build-task:autoprefixer": "postcss dist/css/*.css --use autoprefixer -d dist/css",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar 'src/scss/**/*.scss' -c 'npm run sass:build'",
    "babel": "babel",
    "build": "webpack",
    "prod": "webpack --config build/config.production.js",
    "dev": "webpack --config build/config.development.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-react": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "autoprefixer": "^9.4.7",
    "chokidar-cli": "^1.2.1",
    "d3-array": "^2.0.3",
    "d3-axis": "^1.0.12",
    "d3-dispatch": "^1.0.5",
    "d3-scale": "^2.2.2",
    "d3-selection": "^1.4.0",
    "d3-shape": "^1.3.4",
    "d3-svg-legend": "^2.25.6",
    "d3-transition": "^1.2.0",
    "node-sass": "^4.11.0",
    "node-sass-chokidar": "^1.3.4",
    "node-sass-utils": "^1.1.2",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.1"
  }
}
